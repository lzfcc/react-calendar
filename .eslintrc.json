{
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es2023": true
    },
    "parserOptions": {
      "ecmaVersion": 2023,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": ["error"],
      "no-undef": "error",
      "no-unused-vars": "warn",
      "eqeqeq": "error",
      "camelcase": "off",
      "no-plusplus": ["warn", {"allowForLoopAfterthoughts": true}]
    }
  }