{
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": ["error"]
    }
  }